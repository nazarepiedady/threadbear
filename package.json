{
  "name": "adonis-fullstack-app",
  "version": "4.1.0",
  "adonis-version": "4.1.0",
  "description": "The fullstack application boilerplate for Adonisjs",
  "main": "index.js",
  "scripts": {
    "start": "node server.js",
    "test": "mocha --reporter list",
    "rebuild-db": "adonis migration:refresh && adonis seed",
    "build": "$npm_package_config_webpack",
    "build:watch": "$npm_package_config_webpack -w",
    "build:production": "$npm_package_config_webpack -p"
  },
  "config": {
    "webpack": "webpack --progress --hide-modules --config=./webpack.config.js"
  },
  "keywords": [
    "adonisjs",
    "adonis-app"
  ],
  "author": "",
  "license": "UNLICENSED",
  "private": true,
  "dependencies": {
    "@adonisjs/ace": "^5.0.8",
    "@adonisjs/auth": "^3.0.7",
    "@adonisjs/bodyparser": "~2.0.9",
    "@adonisjs/cors": "^1.0.7",
    "@adonisjs/fold": "^4.0.9",
    "@adonisjs/framework": "^5.0.9",
    "@adonisjs/ignitor": "^2.0.8",
    "@adonisjs/lucid": "^6.3.0",
    "@adonisjs/session": "^1.0.27",
    "@adonisjs/shield": "^1.0.8",
    "@adonisjs/validator": "^5.1.0",
    "currency-map-country": "^1.0.7",
    "geoip-lite": "^1.2.1",
    "js2xmlparser": "^3.0.0",
    "sqlite3": "^5.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.18.2",
    "@babel/preset-env": "^7.18.2",
    "@babel/preset-react": "^7.17.12",
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "bootstrap": "4.6",
    "cheerio": "^1.0.0-rc.10",
    "css-loader": "^6.7.1",
    "laravel-mix": "^6.0.43",
    "mocha": "^9.0.1",
    "node-sass": "^7.0.1",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "resolve-url-loader": "^5.0.0",
    "sass": "^1.51.0",
    "sass-loader": "^13.0.0",
    "url-loader": "^4.1.1",
    "webpack": "^5.73.0"
  },
  "autoload": {
    "App": "./app"
  }
}
